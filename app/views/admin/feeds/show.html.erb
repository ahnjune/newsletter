<h2><%= h @feed.title %></h2>
<h3><%= h @feed.url %></h3>

<h2>Items</h2>

<div class="links">
  <%= link_to "Fetch", fetch_admin_feed_path(@feed) %>
  <%= link_to "Edit", edit_admin_feed_path(@feed) %>
  <%= link_to "Delete", admin_feed_path(@feed), :confirm => "Are you sure?", :method => "delete" %>
  <%= render :partial => "back_to_feeds" %>
</div>