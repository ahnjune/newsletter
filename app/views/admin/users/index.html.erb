<h2>Users</h2>

<%= will_paginate @users %>

<table>
  <tr>
    <th>name</th>
    <th>login</th>
    <th>created</th>
    <th>updated</th>
  </tr>
  <% for user in @users %>
    <tr>
      <td><%= link_to h(user.name), admin_user_path(user) %></td>
      <td><%= h user.login %></td>
      <td><%= datetime user.created_at %></td>
      <td><%= datetime user.updated_at %></td>
    </tr>
  <% end -%>
</table>
